    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "husky": "^4.2.5",
